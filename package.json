{
    "name": "stable-src",
    "version": "1.0.0",
    "description": "Projeto SaaS com frontend React e backend Node.js",
    "private": true,
    "scripts": {
        "postinstall": "npm run setup",
        "setup": "npm run install:frontend && npm run build:frontend && npm run install:backend",
        "install:frontend": "cd frontend && npm install",
        "install:backend": "cd src && npm install",
        "build": "cd frontend && npm run build",
        "dev": "nodemon src/server.js",
        "dev:all": "concurrently \"npm run dev\" \"cd frontend && npm run dev\"",
        "start": "cd src && npm start",
        "test": "cd src && npm test",
        "test:performance": "node scripts/test-performance.js",
        "test:simple": "node scripts/test-simple.js",
        "test:cache": "node scripts/test-simple.js",
        "migrate:consent": "cd src && node scripts/apply_consent_migration_direct.js",
        "migrate:security": "cd src && node scripts/apply_security_migration.js"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "."
    },
    "keywords": [
        "saas",
        "react",
        "nodejs",
        "express",
        "supabase"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.11.0",
        "dotenv": "^17.2.1",
        "node-fetch": "^2.7.0"
    }
}
