# ==============================================
# CONFIGURAÇÕES DO FRONTEND
# ==============================================

# URL da API do Backend
VITE_API_URL=http://localhost:3000

# ==============================================
# CONFIGURAÇÕES DO STRIPE
# ==============================================

# Chave pública do Stripe (obrigatória)
# Obtenha em: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_YOUR_PUBLISHABLE_KEY_chave_publica_de_producao_aqui

# ==============================================
# STRIPE PAYMENT LINKS
# ==============================================

# Premium Plan Payment Links
VITE_STRIPE_PREMIUM_PAYMENT_LINK_MONTHLY=https://buy.stripe.com/SEU_LINK_AQUI
VITE_STRIPE_PREMIUM_PAYMENT_LINK_YEARLY=https://buy.stripe.com/SEU_LINK_AQUI

# Basic Plan Payment Links (opcional)
VITE_STRIPE_BASIC_PAYMENT_LINK_MONTHLY=
VITE_STRIPE_BASIC_PAYMENT_LINK_YEARLY=

# Pro Plan Payment Links (opcional)
VITE_STRIPE_PRO_PAYMENT_LINK_MONTHLY=
VITE_STRIPE_PRO_PAYMENT_LINK_YEARLY=

# ==============================================
# CONFIGURAÇÕES DO FACEBOOK SDK
# ==============================================

# App ID do Facebook (obtido no Facebook Developers)
VITE_APP_META_APP_ID=seu_META_APP_ID_aqui

# Config ID para WhatsApp Business API (obtido no Facebook Developers)
VITE_APP_META_CONFIG_ID=seu_META_APP_CONFIG_ID_aqui

# ==============================================
# CONFIGURAÇÕES DO SUPABASE (se necessário)
# ==============================================

# VITE_SUPABASE_URL=https://seu-projeto.supabase.co
# VITE_SUPABASE_ANON_KEY=sua-chave-anonima-do-supabase

# ==============================================
# CONFIGURAÇÕES DE DESENVOLVIMENTO
# ==============================================

# VITE_DEBUG_MODE=true
# VITE_LOG_LEVEL=debug

# ==============================================
# IMPORTANTE
# ==============================================
# Todas as variáveis do frontend devem começar com VITE_
# As variáveis são expostas no navegador, NÃO inclua dados sensíveis
